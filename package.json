{
  "name": "fuse-jira-client",
  "version": "0.0.1",
  "description": "Mobile Jira client created in Fuse",
  "scripts": {
    "start": "npm run eslint",
    "eslint": "esw -w app/javascript/**/*.js",
    "posteslint": "npm run babel",
    "babel": "babel app/javascript/main.js --out-file javascript/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github:cesarfrick/fuse-jira-client.git"
  },
  "keywords": [
    "Fuse",
    "Jira",
    "ES6",
    "Mobile"
  ],
  "author": "Cesar Frick",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.8.0",
    "eslint-watch": "^2.1.10"
  }
}
